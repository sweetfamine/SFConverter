{
  "manifest_version": 3,
  "name": "Image Format Converter",
  "version": "0.1",
  "description": "Convert PNG, JPG, and WebP.",
  "permissions": ["tabs"],
  "action": {
    "default_title": "Open Converter",
    "default_icon": {
      "16": "icons/arrow-up-down.png",
      "48": "icons/arrow-up-down.png",
      "128": "icons/arrow-up-down.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}